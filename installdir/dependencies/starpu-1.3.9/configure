			if test "$withval" = "yes" ; then
				use_hwloc=yes
			else
				# use specified path
				if test ! -d "$withval" ; then
				   as_fn_error $? "\"Directory specified for hwloc <$withval> does not exist\"" "$LINENO" 5
				fi
				if test ! -d "$withval/lib/pkgconfig" ; then
				   as_fn_error $? "\"Hwloc directory <$withval> does not have a subdirectory lib/pkgconfig\"" "$LINENO" 5
				fi
				export PKG_CONFIG_PATH=$withval/lib/pkgconfig:$PKG_CONFIG_PATH
				use_hwloc=yes
			fi
		else
			use_hwloc=no
		fi

else

		use_hwloc=maybe

fi

