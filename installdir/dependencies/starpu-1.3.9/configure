x|xyes)
  # Assume -lc should be added
  archive_cmds_need_lc_FC=yes

  if test yes,yes = "$GCC,$enable_shared"; then
    case $archive_cmds_FC in
    *'~'*)
      # FIXME: we may have to deal with multi-command sequences.
